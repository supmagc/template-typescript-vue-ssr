{
    "name": "vue-ssr",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "nodemon": "nodemon app/nodemon.js",
        "build:dev:client": "cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=development webpack --config ./build/webpack.client.ts",
        "build:dev:server": "cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=development webpack --config ./build/webpack.server.ts",
        "build:prod:client": "cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=production webpack --config ./build/webpack.client.ts",
        "build:prod:server": "cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=production webpack --config ./build/webpack.server.ts",
        "build:dev": "rimraf ./dist/dev/ & run-p build:dev:client build:dev:server",
        "build:prod": "rimraf ./dist/prod/ & run-p build:prod:client build:prod:server",
        "dev": "run-s build:dev & cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=development ts-node server/main.ts",
        "start": "run-s build:prod & cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=production ts-node server/main.ts"
    },
    "dependencies": {
        "@babel/core": "^7.8.4",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.0.6",
        "vue": "^2.6.11",
        "vue-loader": "^15.8.3",
        "vue-meta": "^2.3.2",
        "vue-router": "^3.1.5",
        "vuex": "^3.1.2",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.2",
        "@types/memory-fs": "^0.3.2",
        "@types/mini-css-extract-plugin": "^0.9.0",
        "@types/webpack": "^4.41.4",
        "@types/webpack-dev-middleware": "^2.0.3",
        "@types/webpack-hot-middleware": "^2.25.0",
        "@types/webpack-merge": "^4.1.5",
        "@types/webpack-node-externals": "^1.7.1",
        "@typescript-eslint/eslint-plugin": "^2.19.0",
        "@typescript-eslint/parser": "^2.19.0",
        "babel-eslint": "^10.0.3",
        "cross-env": "^7.0.0",
        "css-loader": "^3.4.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-vue": "^6.1.2",
        "express": "^4.17.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "memory-fs": "^0.5.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.1",
        "npm-run-all": "^4.1.5",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "prettier": "^1.19.1",
        "sass-loader": "^8.0.2",
        "ts-node": "^8.6.2",
        "typescript": "^3.7.5",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "url-loader": "^3.0.0",
        "vue-eslint-parser": "^7.0.0",
        "vue-hot-reload-api": "^2.3.4",
        "vue-server-renderer": "^2.6.11",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-dev-server": "^3.10.2",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}
