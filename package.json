{
    "name": "vue-ssr",
    "version": "0.1.0",
    "scripts": {
        "build:dev:client": "cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=development webpack --config ./build/webpack.client.ts",
        "build:dev:server": "cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=development webpack --config ./build/webpack.server.ts",
        "build:prod:client": "cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=production webpack --config ./build/webpack.client.ts",
        "build:prod:server": "cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=production webpack --config ./build/webpack.server.ts",
        "build:dev": "rimraf ./dist/dev/ & run-p build:dev:client build:dev:server",
        "build:prod": "rimraf ./dist/prod/ & run-p build:prod:client build:prod:server",
        "dev:server": "cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=development ts-node server/main.ts",
        "dev:nodemon": "nodemon --config build/nodemon.json",
        "dev": "run-s build:dev dev:nodemon",
        "start": "run-s build:prod & cross-env TS_NODE_PROJECT=./build/tsconfig.node.json NODE_ENV=production ts-node server/main.ts"
    },
    "dependencies": {
        "@types/lodash": "^4.14.149",
        "buefy": "^0.8.10",
        "lodash": "^4.17.15",
        "sqlite": "^3.0.3",
        "vue": "^2.6.11",
        "vue-meta": "^2.3.2",
        "vue-property-decorator": "^8.4.0",
        "vue-router": "^3.1.5",
        "vuex": "^3.1.2",
        "vuex-class": "^0.3.2",
        "vuex-module-decorators": "^0.16.1",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "@types/case-sensitive-paths-webpack-plugin": "^2.1.4",
        "@types/express": "^4.17.2",
        "@types/imagemin": "^7.0.0",
        "@types/imagemin-gifsicle": "^5.2.0",
        "@types/imagemin-mozjpeg": "^8.0.0",
        "@types/imagemin-svgo": "^7.0.0",
        "@types/memory-fs": "^0.3.2",
        "@types/mini-css-extract-plugin": "^0.9.0",
        "@types/webpack": "^4.41.4",
        "@types/webpack-dev-middleware": "^2.0.3",
        "@types/webpack-hot-middleware": "^2.25.0",
        "@types/webpack-merge": "^4.1.5",
        "@types/webpack-node-externals": "^1.7.1",
        "@typescript-eslint/eslint-plugin": "^2.19.0",
        "@typescript-eslint/parser": "^2.19.0",
        "case-sensitive-paths-webpack-plugin": "^2.3.0",
        "cross-env": "^7.0.0",
        "css-loader": "^3.4.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-vue": "^6.1.2",
        "express": "^4.17.3",
        "imagemin": "^7.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-pngquant": "^8.0.0",
        "imagemin-svgo": "^7.0.0",
        "memory-fs": "^0.5.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.1",
        "nodemon": "^2.0.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "sass-loader": "^8.0.2",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "typescript": "^3.7.5",
        "url-loader": "^3.0.0",
        "vue-eslint-parser": "^7.0.0",
        "vue-hot-reload-api": "^2.3.4",
        "vue-loader": "^15.8.3",
        "vue-server-renderer": "^2.6.11",
